<h1>Listing archivo_tareas</h1>

<table>
  <thead>
    <tr>
      <th>Archivo</th>
      <th>Tarea</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @archivo_tareas.each do |archivo_tarea| %>
      <tr>
        <td><%= archivo_tarea.archivo_id %></td>
        <td><%= archivo_tarea.tarea_id %></td>
        <td><%= link_to 'Show', archivo_tarea %></td>
        <td><%= link_to 'Edit', edit_archivo_tarea_path(archivo_tarea) %></td>
        <td><%= link_to 'Destroy', archivo_tarea, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Archivo tarea', new_archivo_tarea_path %>
