<h1>Listing user_listtareas</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Listtarea</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user_listtareas.each do |user_listtarea| %>
      <tr>
        <td><%= user_listtarea.user_id %></td>
        <td><%= user_listtarea.listtarea_id %></td>
        <td><%= link_to 'Show', user_listtarea %></td>
        <td><%= link_to 'Edit', edit_user_listtarea_path(user_listtarea) %></td>
        <td><%= link_to 'Destroy', user_listtarea, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User listtarea', new_user_listtarea_path %>
