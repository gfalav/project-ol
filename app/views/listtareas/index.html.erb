<h1>Listing listtareas</h1>

<table>
  <thead>
    <tr>
      <th>Proyecto</th>
      <th>Lista</th>
      <th>Descripcion</th>
      <th>Elista</th>
      <th>Lider</th>
      <th>Falta</th>
      <th>Fvcto</th>
      <th>Fuce</th>
      <th>Fcierre</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @listtareas.each do |listtarea| %>
      <tr>
        <td><%= listtarea.proyecto_id %></td>
        <td><%= listtarea.lista %></td>
        <td><%= listtarea.descripcion %></td>
        <td><%= listtarea.elista %></td>
        <td><%= listtarea.lider_id %></td>
        <td><%= listtarea.falta %></td>
        <td><%= listtarea.fvcto %></td>
        <td><%= listtarea.fuce %></td>
        <td><%= listtarea.fcierre %></td>
        <td><%= link_to 'Show', listtarea %></td>
        <td><%= link_to 'Edit', edit_listtarea_path(listtarea) %></td>
        <td><%= link_to 'Destroy', listtarea, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Listtarea', new_listtarea_path %>
